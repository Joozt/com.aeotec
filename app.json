  "flow": {
    "triggers": [
      {
        "id": "dsa38_press_1",
        "title": {
          "en": "The button has been short pressed",
          "nl": "De knop is kort ingedrukt"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=SDA38"
          }
        ]
      },
      {
        "id": "dsa38_hold_1",
        "title": {
          "en": "The button has been long pressed",
          "nl": "De knop is lang ingedrukt"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=SDA38"
          }
        ]
      }
    ]
  },
  "drivers": [
    {
      "id": "SDA38",
      "name": {
        "en": "Panic Button"
      },
      "class": "sensor",
      "capabilities": [
        "measure_battery"
      ],
      "images": {
        "large": "/drivers/SDA38/assets/images/large.png",
        "small": "/drivers/SDA38/assets/images/small.png"
      },
      "zwave": {
        "manufacturerId": 134,
        "productTypeId": [
          1
        ],
        "productId": [
          38
        ],
        "zwaveAllianceProductId": 1284,
        "learnmode": {
          "instruction": {
            "en": "Press the \"Learn\" button on the Panic Button.",
            "nl": "Druk de \"Learn\" knop op de Panic Button in."
          }
        },
        "associationGroups": [
          1
        ],
        "defaultConfiguration": [
          {
            "id": 250,
            "size": 1,
            "value": 1
          }
        ]
      }
    }
  ]
}