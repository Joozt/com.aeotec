  "drivers": [
    {
      "id": "DSB45",
      "name": {
        "en": "Water Sensor"
      },
      "class": "sensor",
      "capabilities": [
        "measure_battery",
        "alarm_water"
      ],
      "images": {
        "large": "/drivers/DSB45/assets/images/large.png",
        "small": "/drivers/DSB45/assets/images/small.png"
      },
      "zwave": {
        "manufacturerId": 134,
        "productTypeId": [
          2
        ],
        "productId": [
          45
        ],
        "learnmode": {
          "image": "/drivers/DSB45/assets/learnmode.svg",
          "instruction": {
            "en": "Press the action button on the Water Sensor",
            "nl": "Druk op de actie knop op de Water Sensor"
          }
        },
        "associationGroups": [
          1
        ],
        "defaultConfiguration": [
          {
            "id": 121,
            "size": 4,
            "value": 299
          }
        ]
      },
      "settings": [
        {
          "id": "1",
          "type": "radio",
          "label": {
            "en": "Operation mode",
            "nl": "Werking"
          },
          "hint": {
            "en": "This parameter (1) determines whether the alarm goes off when water is detected or when no water is detected.",
            "nl": "Deze parameter (1) bepaalt of het alarm afgaat als er water gedetecteert wordt of wanneer er geen water gedetecteert wordt."
          },
          "zwave": {
            "index": 1,
            "size": 1
          },
          "value": "0",
          "values": [
            {
              "id": "0",
              "label": {
                "en": "Warn when water detected",
                "nl": "Waarschuw als er water gedetecteert wordt"
              }
            },
            {
              "id": "1",
              "label": {
                "en": "Warn when no water detected",
                "nl": "Waarschuw als er geen water gedetecteert wordt"
              }
            }
          ]
        },
        {
          "id": "121",
          "type": "dropdown",
          "label": {
            "en": "Information to send",
            "nl": "Te versturen informatie"
          },
          "hint": {
            "en": "This parameter (121) determines what kind of report will be sent to the associated nodes when the Water Sensor detects water.",
            "nl": "Deze parameter (121) bepaalt wat voor soort rapport er wordt verzonder naar de geassocieerde apparaten wanneer de Water Sensor water detecteert."
          },
          "zwave": {
            "index": 121,
            "size": 4
          },
          "value": "299",
          "values": [
            {
              "id": "0",
              "label": {
                "en": "Nothing",
                "nl": "Niets"
              }
            },
            {
              "id": "298",
              "label": {
                "en": "Water detect",
                "nl": "Water detectie"
              }
            },
            {
              "id": "299",
              "label": {
                "en": "Water detect and battery",
                "nl": "Water detectie and batterij"
              }
            }
          ]
        }
      ]
    }
  ]
}